	@echo "...Installing translation files..."
	@#
	@echo "   af_ZA ...Afrikaans..."
	@install -d $(LOCALE_PREFIX)/af/LC_MESSAGES
	@cp trans/af.mo $(LOCALE_PREFIX)/af/LC_MESSAGES/tuxpaint.mo
	@chmod 644 $(LOCALE_PREFIX)/af/LC_MESSAGES/tuxpaint.mo
	@#
	@echo "   sq_AL ...Albanian..."
